#include "states.h"

//---- INSERTION SORT ALGORITMASI ----//
void insertion(int array[],int size){
    int i,j,temp;

    for(i=0;i<size;i++){
        for(j=0;j<i+1;j++){
            if(array[j]>array[i]){
                temp=array[i];
                array[i]=array[j];
                array[j]=temp;
            }
        }
    }

    for(i=0;i<size;i++)
        printf("%d ",array[i]);
}


void print_array(uint8_t array[],uint8_t size){
	uint8_t i;
	for(i=0;i<size;i++){
		printf("%d",)
	}
}

